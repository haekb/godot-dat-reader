	}
	else if(n_out12p0 < n_in11p1)
	{
		n_out11p0 = n_out7p0;
	}
	else
	{
		n_out11p0 = n_out7p0;
	}

// VectorOp:6
	vec3 n_out6p0 = n_out2p0 * n_out11p0;

// Output:0
	ALBEDO = n_out6p0;

}

